mage {\n __typename\n fullSize\n }\n }\n }\n }\n}\nfragment PhotoPosterDetails on PhotoPoster {\n __typename\n ... on StrangerUser {\n __typename\n ...StrangerUserDetails\n }\n ... on ExternalUser {\n __typename\n ...ExternalUserDetails\n }\n}\nfragment StrangerUserDetails on StrangerUser {\n __typename\n id\n firstName\n profilePicture {\n __typename\n ...ImageDetails\n }\n}\nfragment ExternalUserDetails on ExternalUser {\n __typename\n id\n name\n profilePicture {\n __typename\n ...ImageDetails\n }\n}\nfragment StrangerWalkDetails on StrangerWalk {\n __typename\n id\n distance\n stepCount\n liked\n likeCount\n}\nfragment PageInfoDetails on PageInfo {\n __typename\n startCursor\n endCursor\n hasNextPage\n hasPreviousPage\n}"

DOG_ICON = base64.decode("""

""")


def format_time(minutes):
    if minutes < MINUTES_IN_HOUR:
        return "{}m".format(minutes)
    hours_total = minutes // MINUTES_IN_HOUR

    # Get additional minutes with modulus
    minutes_total = minutes % MINUTES_IN_HOUR
    return "{}h{}m".format(hours_total, minutes_total)


def main(config):
    populate_login_cache(config)
    auth = fi_login(cache.get("email"), cache.get("password"))

    session_id = auth.get('session_id')
    cookie = auth.get('cookie')

    if not session_id or not cookie or not config.get("pet"):
        return render.Root(
            child=render.Text(
                content="Missing Data",
                font="6x13",
                color=WHITE,
            ),
        )

    daily_stats = fi_pet_stats(config.get("pet"))
    pets = fi_get_pets()
    pet_data = None
    for pet in pets:
        if str(pet["id"]) == config.get("pet"):
            pet_data = pet
            break

    percent_activity_complete = daily_stats.get(
        "totalSteps",
        0,
    ) / daily_stats["stepGoal"]
    width_complete = math.floor(percent_activity_complete * PROGRESS_BAR_WIDTH)
    if width_complete > PROGRESS_BAR_WIDTH:
        width_complete = PROGRESS_BAR_WIDTH

    width_left = PROGRESS_BAR_WIDTH - width_complete

    animations = []

    for i in range(width_complete):
        animations.append(
            animation.Keyframe(
                percentage=(i + 1) / width_complete,
                transforms=[animation.Scale(y=1, x=(i + 1) * 2)],
            ),
        )

    progress_bar = [
        render.Box(
            width=1,
            height=PROGRESS_BAR_HEIGHT,
            color=WHITE,
        ),
    ]

    if width_complete:
        progress_bar.append(
            animation.Transformation(
                height=PROGRESS_BAR_HEIGHT,
                width=width_complete,
                child=render.Box(
                    width=1,
                    height=PROGRESS_BAR_HEIGHT,
                    color=FILLED_PROGRESS_BAR,
                ),
                duration=PROGRESS_BAR_ANIMATION_DURATION,
                delay=0,
                keyframes=animations,
            ),
        )
    if width_left:
        progress_bar.append(
            render.Box(
                width=width_left,
                height=PROGRESS_BAR_HEIGHT,
                color=LEFT_PROGRESS_BAR,
            ),
        )
    progress_bar.append(
        render.Padding(
            pad=(0, 0, 1, 0),
            color=BLACK,
            child=render.Box(
                width=1,
                height=PROGRESS_BAR_HEIGHT,
                color=WHITE,
            ),
        ),
    )

    return render.Root(
        child=render.Column(
            children=[
                render.Row(
                    main_align="space_between",
                    expanded=True,
                    children=[
                        render.Row(
                            children=[
                                render.Text(pet_data["name"]),
                                render.Image(src=DOG_ICON),
                            ],
                        ),
                        render.Text(
                            "{}%".format(
                                pet_data["device"]['info']["batteryPercent"],
                            ),
                        ),
                    ],
                ),
                render.Row(
                    main_align="space_between",
                    expanded=True,
                    children=[
                        render.Column(
                            children=[
                                render.Box(
                                    width=PROGRESS_BAR_CONTAINER_WIDTH,
                                    height=1,
                                    color=WHITE,
                                ),
                                render.Row(
                                    children=progress_bar,
                                ),
                                render.Box(
                                    width=PROGRESS_BAR_CONTAINER_WIDTH,
                                    height=1,
                                    color=WHITE,
                                ),
                            ],
                        ),
                    ],
                ),
                render.Row(
                    cross_align="center",
                    children=[
                        render.Text("{} Steps".format(
                            humanize.comma(daily_stats.get("totalSteps", 0))
                        ), font="tb-8"),
                    ],
                ),
                render.Row(
                    cross_align="center",
                    children=[
                        render.Text(humanize.comma(
                            math.floor((daily_stats.get("totalDistance", 0) / MILES_TO_METERS_CONVERSION) * 100) / 100
                        )),
                        render.Text("mi", font="tb-8"),
                    ],
                ),
            ],
        ),
    )


def wait(seconds, passed_time = None):
    # print('waiting')
    if passed_time:
        duration = time.now() - passed_time
        # print(duration)
        if  duration.seconds >= seconds:
            return
        else:
            wait(seconds, passed_time)
    else:
        wait(seconds, time.now())

# buildifier: disable=function-docstring

def fi_login(email, password):
    if not email or not password:
        return {}
    session_id = cache.get("session")
    cookie = cache.get("cookie")
    if session_id and cookie:
        return {
            "session_id": session_id,
            "cookie": cookie,
        }

    res = http.post(
        FI_API + "/auth/login",
        json_body=dict(
            email=email,
            password=password,
        ),
    )

    print(res)
    wait(0)

    if res.status_code >= 300:
        # buildifier: disable=print
        print("fi_auth_call failed: {} - {} ".format(res.status_code, res.body()))
        return None

    session_id = res.json()["sessionId"]
    cookie = res.headers["Set-Cookie"]
    cache.set("session_id", session_id)
    cache.set("cookie", cookie)

    return {
        "session_id": session_id,
        "cookie": cookie,
    }


# buildifier: disable=function-docstring

def fi_api_call(query, use_cache=True):
    auth = fi_login(cache.get("email"), cache.get("password"))
    session_id = auth['session_id']
    cookie = auth['cookie']
    if not session_id or not cookie:
        return fail("No session id")

    cache_key = "{}|{}".format(session_id, json.encode(query))
    cached_res = cache.get(cache_key) if use_cache else None
    if not cached_res:
        res = http.post(
            FI_API + "/graphql",
            headers={
                "Session": session_id,
                "Cookie": cookie,
            },
            json_body={
                "query": query,
            }

        )
        if res.status_code != 200:
            # buildifier: disable=print
            print("fi_api_call failed: {} - {} ".format(res.status_code, res.body()))
            return None
        cached_res = res.body()
        if use_cache:
            cache.set(cache_key, cached_res, DEFAULT_CACHE_TTL)
    return json.decode(cached_res)


def fi_get_pets():
    qString = QUERY_CURRENT_USER_FULL_DETAIL + FRAGMENT_USER_DETAILS \
              + FRAGMENT_USER_FULL_DETAILS + FRAGMENT_PET_PROFILE + FRAGEMENT_BASE_PET_PROFILE \
              + FRAGMENT_BASE_DETAILS + FRAGMENT_POSITION_COORDINATES + FRAGMENT_BREED_DETAILS \
              + FRAGMENT_PHOTO_DETAILS + FRAGMENT_DEVICE_DETAILS + FRAGMENT_LED_DETAILS + FRAGMENT_OPERATIONAL_DETAILS \
              + FRAGMENT_CONNECTION_STATE_DETAILS

    response = fi_api_call(qString)
    return response['data']['currentUser']['userHouseholds'][0]['household']['pets']


def fi_pet_stats(pet_id):
    qString = QUERY_PET_ACTIVITY.replace(VAR_PET_ID, pet_id) + FRAGMENT_ACTIVITY_SUMMARY_DETAILS
    response = fi_api_call(qString)
    return response['data']['pet']['dailyStat']


def fi_get_pet_daily_time(pet_id):
    qString = QUERY_PET_ACTIVITY.replace(VAR_PET_ID, pet_id) + FRAGMENT_ACTIVITY_SUMMARY_DETAILS
    response = fi_api_call(qString)
    return response['data']['pet']['dailyStat']


def show_pet_options(password):
    if password and cache.get("email"):
        # populate the cache with auth information
        fi_login(cache.get("email"), password)
        pets = fi_get_pets()

        pet_options = []
        for pet in pets:
            pet_options.append(
                schema.Option(
                    display=pet["name"],
                    value=str(pet["id"]),
                ),
            )

        return [
            schema.Dropdown(
                id="pet",
                name="Pet",
                desc="The pet you want information on.",
                default=pet_options[0].value,
                options=pet_options,
                icon="dog",
            ),
        ]


def populate_login_cache(config):
    if config.get("email"):
        cache.set("email", config.get("email"))
    if config.get("password"):
        cache.set("password", config.get("password"))


def get_schema():
    return schema.Schema(
        version="1",
        fields=[
            schema.Text(
                id="email",
                name="Email",
                desc="Email of your Fi Account.",
                icon="gear",
            ),
            schema.Text(
                id="password",
                name="Password",
                desc="Password of your Fi Account.",
                icon="gear",
            ),
            schema.Generated(
                id="show_pets",
                source="password",
                handler=show_pet_options,
            ),
        ],
    )
