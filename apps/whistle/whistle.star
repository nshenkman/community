def format_time(minutes):
    if minutes < 60:
        return str(minutes) + "m"
    hours_total = minutes // 60
    # Get additional minutes with modulus
    minutes_total = minutes % 60
    return str(hours_total) + "h" + str(minutes_total) + "m"


def main(config):
    populate_login_cache(config)
    auth = whistle_login(cache.get("email"), cache.get("password"))

    if not auth and not config.get("pet"):
        return render.Root(
            child=render.Text(
                content="Missing Data",
                font="6x13",
                color="#fff",
            ),
        )

    daily_stats = whistle_get_pet_stats(config.get("pet"))
    pets = whistle_get_pets()
    pet_data = None
    for pet in pets:
        if str(pet["id"]) == config.get("pet"):
            pet_data = pet
            break

    percent_activity_complete = daily_stats.get(
        "minutes_active", 0) / daily_stats["activity_goal"]
    width_complete = math.floor(percent_activity_complete * 30)
    if width_complete > 30:
        width_complete = 30

    width_left = 30 - width_complete

    animations = []

    for i in range(width_complete):
        animations.append(
            animation.Keyframe(
                percentage=(i+1)/width_complete,
                transforms=[animation.Scale(y=1, x=(i+1) * 2)],
            ),
        )

    progress_bar = [
        render.Box(
            width=1, height=6, color="#ffffff"
        ),
        animation.Transformation(
            height=6,
            width=width_complete,
            child=render.Box(width=1, height=6, color="#00FF00"),
            duration=50,
            delay=0,
            keyframes=animations,
        ),

    ]

    if width_left:
        progress_bar.extend([
            render.Box(
                width=width_left, height=6, color="#565656"
            ),
            render.Box(
                width=1, height=6, color="#ffffff"
            ),
        ])
    else:
        progress_bar.append(
            render.Box(
                width=1, height=6, color="#ffffff"
            ),
        )
    return render.Root(
        child=render.Column(
            children=[
                render.Row(
                    cross_align="center",
                    children=[
                        render.Text(" %s " % pet_data["name"]),
                        render.Image(src=DOG_ICON),
                        render.Text(
                            " %s" % (str(pet_data["device"]["battery_level"]) + "%")),
                    ],
                ),
                render.Row(
                    cross_align="center",
                    children=[
                        render.Column(
                            children=[
                                render.Row(
                                    children=[
                                        render.Text(" %s " % format_time(
                                            daily_stats.get("minutes_active", 0)),  font="tb-8"),
                                    ]
                                )
                            ]
                        ),
                        render.Column(
                            children=[
                                render.Box(
                                    width=32, height=1, color="#ffffff"
                                ),
                                render.Row(
                                    children=progress_bar,
                                ),
                                render.Box(
                                    width=32, height=1, color="#ffffff"
                                ),
                            ]
                        )
                    ],
                ),
                render.Row(
                    cross_align="center",
                    children=[
                        render.Text(" %s " % humanize.comma(
                            float(daily_stats.get("distance", 0)))),
                        render.Text("mi", font="tb-8"),
                    ],
                ),
                render.Row(
                    cross_align="center",
                    children=[
                        render.Text(" %s kcal " % math.floor(
                            daily_stats.get("calories", 0)), font="tb-8"),
                    ],
                ),
            ],
        ),
    )


# buildifier: disable=function-docstring
def whistle_login(email, password):
    if not email or not password:
        return None
    auth = cache.get("auth")
    if auth:
        return json.decode(auth)

    res = http.post(
        WHISTLE_API + "/login",
        json_body=dict(
            email=email,
            password=password,
        ),
    )

    if res.status_code >= 300:
        # buildifier: disable=print
        print("whistle_auth_call failed: %s - %s " %
              (res.status_code, res.body()))
        return None

    auth = res.body()
    cache.set("auth", auth)

    return json.decode(auth)


def whistle_get_pets():
    pets = whistle_api_call("/pets")

    return pets["pets"]


def whistle_get_pet_stats(pet_id):
    dailies = whistle_api_call("/pets/" + pet_id + "/dailies")
    today = dailies["dailies"][0]

    return today


# buildifier: disable=function-docstring
def whistle_api_call(url, use_cache=True):
    auth = whistle_login(cache.get('email'), cache.get('password'))
    if not auth:
        return fail("No access token")

    auth_token = auth["auth_token"]
    cache_key = "%s|%s" % (auth_token, url)
    cached_res = cache.get(cache_key) if use_cache else None

    if not cached_res:
        res = http.get(
            WHISTLE_API + url,
            headers={
                "Host": "app.whistle.com",
                "Content-Type": "application/json",
                "Connection": "keep-alive",
                "Accept-Language": "en",
                "Accept-Encoding": "br, gzip, deflate",
                "Authorization": "Bearer " + auth_token,
                "Accept": "application/vnd.whistle.com.v5+json",
                "User-Agent": "Winston/3.9.0 (iPhone; iOS 13.5; Build:2399; Scale/3.0)",
            },
        )

        if res.status_code != 200:
            # buildifier: disable=print
            print("whistle_api_call failed: %s - %s " %
                  (res.status_code, res.body()))
            return None

        cached_res = res.body()

        if use_cache:
            cache.set(cache_key, cached_res, DEFAULT_CACHE_TTL)

    return json.decode(cached_res)


def show_pet_options(password):
    if password and cache.get("email"):
        # populate the cache with auth information
        whistle_login(cache.get("email"), password)

        pets = whistle_get_pets()
        pet_options = []
        for pet in pets:
            pet_options.append(
                schema.Option(
                    display=pet["name"],
                    value=str(pet["id"]),
                )
            )
        return [
            schema.Dropdown(
                id="pet",
                name="Pet",
                desc="The pet you want information on.",
                default=pet_options[0].value,
                options=pet_options,
                icon="dog"
            )
        ]


def populate_login_cache(config):
    if config.get("email"):
        cache.set("email", config.get('email'))
    if config.get('password'):
        cache.set("password", config.get('password'))


def get_schema():
    return schema.Schema(
        version="1",
        fields=[
            schema.Text(
                id="email",
                name="Email",
                desc="email of your Whistle Account.",
                icon="gear",
            ),
            schema.Text(
                id="password",
                name="Password",
                desc="Password of your Whistle Account.",
                icon="gear",
            ),
            schema.Generated(
                id="show_pets",
                source="password",
                handler=show_pet_options,
            ),
        ],
    )
